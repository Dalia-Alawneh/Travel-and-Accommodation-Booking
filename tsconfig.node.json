{
  "compilerOptions": {
    "types": ["vitest/globals", "@testing-library/jest-dom"],
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": "src",
    "paths": {
      "@travelia/*": ["./src/*"]
    }
  },
  "include": ["vite.config.ts"]
}
